{
  "compilerOptions": {
    "outDir": "dist",
    "module": "commonjs",
    "target": "es2019",
    "sourceMap": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strictNullChecks": false,              
    "forceConsistentCasingInFileNames": true,
    "rootDir": "src",
    "baseUrl": "src",
    "paths":{
      "@/*":["*"]
    }
  },
  "include": [
    "src"
  ],
  "exclude": []
}